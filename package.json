{
  "name": "@erikmaday/unusual-whales-mcp",
  "version": "0.3.0",
  "description": "MCP server for UnusualWhales API - access options flow, dark pool, congress trades, and more",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "unusual-whales-mcp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikmaday/unusual-whales-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/erikmaday/unusual-whales-mcp/issues"
  },
  "homepage": "https://github.com/erikmaday/unusual-whales-mcp#readme",
  "license": "MIT",
  "author": "Erik Maday",
  "keywords": [
    "mcp",
    "unusual-whales",
    "options",
    "trading",
    "dark-pool",
    "congress-trades",
    "ai",
    "llm"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "dev:inspector": "npx @modelcontextprotocol/inspector node dist/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "fetch-spec": "node scripts/fetch-api-spec.js",
    "check-api": "node scripts/check-api-sync.js",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest",
    "test:watch:unit": "vitest tests/unit",
    "test:coverage": "vitest run --coverage",
    "coverage:open": "vitest run --coverage && open coverage/index.html",
    "validate": "npm run lint && npm run build && npm test",
    "prepare": "husky"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.7",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.51.0",
    "vitest": "^4.0.17",
    "yaml": "^2.8.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  }
}
